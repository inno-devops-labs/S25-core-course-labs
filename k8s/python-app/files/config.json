{
  "appName": "Moscow Time Display",
  "version": "1.0.0",
  "environment": "production",
  "timezone": "Europe/Moscow",
  "features": {
    "timeDisplay": true,
    "apiEndpoints": true,
    "visitsEndpoint": true
  },
  "logging": {
    "level": "info",
    "format": "json",
    "enableConsole": true
  },
  "performance": {
    "caching": true,
    "cacheTTL": 3600
  }
} 